{
  "name": "queue-ui",
  "version": "0.0.1",
  "description": "BullMQ Queue Monitoring Dashboard for WellFlow",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "check-types": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:coverage:watch": "jest --coverage --watch --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:check": "pnpm run security:audit && pnpm run lint",
    "security:test": "pnpm run security:audit && pnpm run lint",
        "accessibility:test": "echo 'Accessibility testing not applicable for queue monitoring service'"
  },
  "jest": {
    "displayName": "Queue UI",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "transpilation": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.d.ts",
      "!**/*.config.ts",
      "!**/index.ts",
      "!**/test/**",
      "!**/__tests__/**",
      "!**/*.spec.ts",
      "!**/*.test.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "html",
      "json",
      "json-summary",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../jest.setup.js"
    ],
    "testTimeout": 30000,
    "verbose": true,
    "clearMocks": true,
    "restoreMocks": true,
    "forceExit": true,
    "detectOpenHandles": false
  }
},
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "bullmq": "^5.58.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "8.1.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "bullmq",
    "queue",
    "monitoring",
    "dashboard",
    "oil-gas",
    "wellflow"
  ],
  "author": "WellFlow Team",
  "license": "UNLICENSED"
}
