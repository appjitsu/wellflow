# Queue UI Dashboard Configuration

# Server Configuration
PORT=3003
NODE_ENV=development

# Redis Configuration (Railway will provide this automatically)
REDIS_URL=redis://localhost:6379

# JWT Authentication (must match main API)
JWT_SECRET=your-jwt-secret-here

# API Configuration
API_BASE_URL=http://localhost:3001

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,https://your-domain.com

# Railway Environment Variables (automatically provided)
# RAILWAY_ENVIRONMENT=production
# RAILWAY_SERVICE_NAME=queue-ui
# RAILWAY_PROJECT_NAME=wellflow

# Security Configuration
HELMET_CSP_ENABLED=true

# Logging Configuration
LOG_LEVEL=info
